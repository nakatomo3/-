        <Trigger>
            <ConnectNumber>1</ConnectNumber>
            <Type>Button</Type>
            <X>2</X>
            <Y>-1</Y>
        </Trigger>
    
        <Parts>
            <ConnectNumber>1</ConnectNumber>
            <Name>Door</Name>
            <X>0</X>
            <Y>0</Y>
        </Parts>
    </Gimmick>

    <Trigger>
        <ConnectNumber>2</ConnectNumber>
        <Type>RightGear</Type>
        <X>-2</X>
        <Y>2</Y>
    </Trigger>


    <!-- ゲームに影響のない見た目だけの部分。　未実装-->
    <BackGround>
        <Name></Name>
        <X>0</X>
        <Y>0</Y>
    </BackGround>

</Stage>